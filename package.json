{
	"name": "whereami",
	"private": true,
	"version": "2.0.4",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && cp -r functions .svelte-kit/cloudflare/",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"deploy": "npm run build && npx wrangler pages deploy .svelte-kit/cloudflare --project-name whereami",
		"deploy:prod": "./deploy.sh production",
		"deploy:dev": "./deploy.sh development",
		"dev:build": "npm run build",
		"dev:functions": "npm run build && wrangler pages dev .svelte-kit/cloudflare --d1 DB --r2 IMAGES_BUCKET --kv IMAGE_DATA --kv USER_DATA --kv GAME_DATA --port 8788",
		"wrangler": "wrangler"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@playwright/test": "^1.52.0",
		"@sveltejs/adapter-cloudflare": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/esri-leaflet": "^3.0.3",
		"dotenv": "^16.5.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"wrangler": "^4.18.0"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.50.0",
		"@types/leaflet": "^1.9.18",
		"esri-leaflet": "^3.0.16",
		"esri-leaflet-vector": "^4.3.0",
		"exifr": "^7.1.3",
		"leaflet": "^1.9.4"
	}
}
